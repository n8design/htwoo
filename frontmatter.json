{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "link",
          "name": "link",
          "type": "string"
        },
        {
          "title": "logo",
          "name": "logo",
          "type": "image"
        },
        {
          "title": "tagline",
          "name": "tagline",
          "type": "string"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313/htwoo/",
  "frontMatter.content.pageFolders": [
    {
      "title": "content",
      "path": "[[workspace]]/content"
    },
    {
      "title": "guidelines",
      "path": "[[workspace]]/content/guidelines"
    },
    {
      "title": "how-to",
      "path": "[[workspace]]/content/how-to"
    },
    {
      "title": "Supporters",
      "path": "[[workspace]]/content/supporters"
    }
  ]
}